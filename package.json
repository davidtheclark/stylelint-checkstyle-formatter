{
    "name": "stylelint-checkstyle-reporter",
    "version": "0.2.0",
    "description": "Output Checkstyle XML reports of stylelint results",
    "main": "index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc --project tsconfig.app.json",
        "format": "prettier -w ./**/*.{json,ts}",
        "lint": "npm run lint:prettier && npm run lint:eslint",
        "lint:eslint": "npx eslint {src,test}/**/*.ts",
        "lint:eslint:fix": "npx eslint {src,test}/**/*.ts",
        "lint:prettier": "npx prettier --check \"./**/*.{json,scss,ts,js,md}\"",
        "lint:prettier:fix": "npx prettier --write \"./**/*.{json,scss,ts,js,md}\"",
        "prepack": "npm run build",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lucavb/stylelint-checkstyle-reporter.git"
    },
    "keywords": [
        "stylelint",
        "checkstyle",
        "formatter",
        "reporter",
        "report"
    ],
    "author": {
        "name": "Luca Becker",
        "email": "luca.becker@sunbury.xyz",
        "url": "https://sunbury.xyz"
    },
    "license": "MIT",
    "files": [
        "dist",
        "index.js"
    ],
    "bugs": {
        "url": "https://github.com/lucavb/stylelint-checkstyle-reporter/issues"
    },
    "homepage": "https://github.com/lucavb/stylelint-checkstyle-reporter#readme",
    "dependencies": {
        "xmlbuilder2": "2.4.0"
    },
    "devDependencies": {
        "@types/jest": "26.0.19",
        "@types/node": "14.14.16",
        "@types/stylelint": "9.10.1",
        "@typescript-eslint/eslint-plugin": "4.11.0",
        "@typescript-eslint/parser": "4.10.0",
        "eslint": "7.16.0",
        "eslint-config-google": "0.14.0",
        "eslint-config-prettier": "7.1.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-prettier": "3.3.0",
        "eslint-plugin-unused-imports": "1.0.1",
        "jest": "26.6.3",
        "prettier": "2.2.1",
        "prettier-eslint": "12.0.0",
        "rxjs": "6.6.3",
        "stylelint": "13.8.0",
        "stylelint-config-recommended-scss": "4.2.0",
        "stylelint-config-standard": "20.0.0",
        "stylelint-scss": "3.18.0",
        "ts-jest": "26.4.4",
        "typescript": "4.1.3"
    }
}
