{
    "env": {
        "es2021": true,
        "node": true
    },
    "plugins": ["@typescript-eslint", "prettier"],
    "extends": ["airbnb-base", "prettier", "prettier/@typescript-eslint"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": ["error"]
    }
}
